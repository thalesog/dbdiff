{
  "name": "@thalesog/dbdiff",
  "version": "1.0.0",
  "description": "Compares two databases and prints SQL commands to modify the first one in order to match the second one. This is a improved fork from dbdiff",
  "files": [
    "!lib/__tests__/**/*",
    "lib/**/*",
    "bin/**/*"
  ],
  "bin": {
    "dbdiff": "./bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "coverage": "codecov",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thalesog/dbdiff.git"
  },
  "license": "MIT",
  "author": {
    "name": "Thales ogliari",
    "email": "thalesog@users.noreply.github.com",
    "url": "https://github.com/thalesog"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "pg",
    "pgsql",
    "postgresql",
    "mysql",
    "database",
    "diff",
    "database",
    "compare"
  ],
  "bugs": {
    "url": "https://github.com/thalesog/dbdiff/issues"
  },
  "homepage": "https://github.com/thalesog/dbdiff#readme",
  "dependencies": {
    "commander": "^7.2.0",
    "mysql2": "^2.3.2",
    "parse-database-url": "^0.3.0",
    "pg": "^8.7.1",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.11",
    "@types/pg": "^8.6.1",
    "@types/underscore": "^1.11.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chalk": "^4.1.1",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "^5.1.1",
    "husky": "^6.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.7",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
